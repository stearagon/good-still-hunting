<!DOCTYPE html>
<html>
<head>
  <title>GoodStillHunting</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if !flash[:errors].nil? %>
    <% flash[:errors].each do |error| %>
      <%= error %>
    <% end %>
  <% end %>
<%= yield %>

</body>
</html>
