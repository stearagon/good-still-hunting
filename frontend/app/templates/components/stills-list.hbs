{{#each stills as |still|}}
  <div class="stills-list__still">
    {{#link-to 'dashboard.stills.still' still.id}}
      <img src="{{still.medium_image_url}}"/>
    {{/link-to}}
  </div>
{{/each}}

{{#if endOfList}}
  <div>End of stills</div>
{{/if}}

<span {{action 'onLoadNext'}} class='load-button'></span>

{{#if isLoading}}
  <div class="modal">
    {{ember-spinner
      lines=12
      radius=10
      width=5
      rotate=0
      speed='3.4'
      zIndex='5000'
      color='orange'}}
  </div>
{{/if}}
